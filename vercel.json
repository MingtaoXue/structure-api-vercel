{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/openapi.json",
      "dest": "/api/openapi.json"
    },
    {
      "src": "/logo.png",
      "dest": "/api/logo.png"
    },
    {
      "src": "/privacy.md",
      "dest": "/api/privacy.md"
    },
    {
      "src": "/.*",
      "dest": "api/index.py"
    }
  ]
}
